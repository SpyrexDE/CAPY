func _input(event: InputEvent) -> void:
	if Input.is_action_just_pressed(\"zoom_in\"):
		zoom += Vector2(0.1, 0.1)
	elif Input.is_action_just_pressed(\"zoom_out\"):
		if zoom - Vector2(0.1, 0.1) > Vector2.ZERO:
			zoom -= Vector2(0.1, 0.1)
"

