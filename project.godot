boot_splash/bg_color=Color(0.027451, 0.0470588, 0.12549, 1)
config/icon="res://src/items/melon_seed/melon_seed.png"

[autoload]

Game="*res://src/global/autoload/game.gd"
Utils="*res://src/global/autoload/utils.gd"
